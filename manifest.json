{
	"manifest_version": 2,
	"name": "Tabulance",
	"version": "1.0",
	"description": "Adds a red border to all webpages matching mozilla.org.",
	"icons": {
		"48": "assets/icons/icon-48.png"
	},
	"content_scripts": [
		{
			"matches": [
				"*://*.mozilla.org/*"
			],
			"js": [
				"main.js"
			]
		}
	]
}
